{
  "name": "@web/test-runner-commands",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Web test runner commands",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/modernweb-dev/web.git",
    "directory": "packages/test-runner-commands"
  },
  "author": "modern-web",
  "homepage": "https://github.com/modernweb-dev/web/tree/master/packages/test-runner-commands",
  "main": "dist/index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "mocha test/**/*.test.ts --require ts-node/register --reporter progress",
    "test:ci": "yarn test",
    "test:watch": "mocha test/**/*.test.ts --require ts-node/register --watch --watch-files src,test --reporter progress"
  },
  "files": [
    "browser",
    "dist"
  ],
  "keywords": [
    "web",
    "test",
    "runner",
    "testrunner",
    "commands"
  ],
  "dependencies": {
    "@web/test-runner-core": "^0.6.21",
    "@web/test-runner-server": "^0.5.14"
  }
}
