{
  "name": "@web/storybook-builder",
  "version": "0.1.6",
  "publishConfig": {
    "access": "public"
  },
  "description": "Storybook builder powered by `@web/dev-server`",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/modernweb-dev/web.git",
    "directory": "packages/storybook-builder"
  },
